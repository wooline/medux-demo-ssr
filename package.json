{
  "name": "medux-demo-ssr",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "stylelint": "stylelint \"./src/**/*.less\" --syntax less",
    "eslint": "eslint \"./src/**/*.ts\"",
    "types-check": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run types-check",
    "start": "npm run lint && node build/start.js",
    "build": "npm run lint && node build/build.js",
    "site": "npm run lint && cross-env WEBSITE=site node build/build.js",
    "build-demo": "node build/ssr-demo.js && npm run build && node build/express-demo.js"
  },
  "repository": "https://github.com/wooline/medux-demo-ssr",
  "homepage": "https://github.com/wooline/medux-demo-ssr",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "not IE 11"
  ],
  "devServer": {
    "url": "http://localhost:7445",
    "mock": true,
    "ssr": true,
    "proxy": {
      "/ajax/**": {
        "target": "http://localhost:7446/",
        "secure": false,
        "changeOrigin": true
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "~7.4.5",
    "@medux/react-web-router": "0.1.8",
    "antd-mobile": "^2.2.13",
    "core-js": "~3.1.3",
    "indexof": "0.0.1",
    "rc-form": "^2.4.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@medux/dev-utils": "0.1.3",
    "@medux/dev-pkg": "0.1.3",
    "babel-plugin-import": "^1.12.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "typescript": "~3.5.3"
  }
}
