{
  "name": "medux-demo-ssr",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "stylelint": "stylelint \"./src/**/*.less\" --syntax less",
    "eslint": "eslint \"./src/**/*.ts\"",
    "types-check": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run types-check",
    "start": "npm run lint && cross-env NODE_ENV=development SITE=local node build/start.js",
    "build": "npm run lint && cross-env NODE_ENV=production SITE=local node build/build.js",
    "site": "npm run lint && cross-env WEBSITE=site node build/build.js",
    "build-demo": "node build/ssr-demo.js && npm run build && node build/express-demo.js"
  },
  "repository": "https://github.com/wooline/medux-demo-ssr",
  "homepage": "https://github.com/wooline/medux-demo-ssr",
  "author": "wooline <wooline@qq.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "browserslist": [
    "ie >= 9"
  ],
  "baseConf": {
    "siteName": "Medux",
    "clientPublicPath": "/",
    "server": "http://localhost:7445",
    "apiServer": "http://localhost:7446",
    "mock": true,
    "ssr": true,
    "proxy": {
      "/ajax/**": {
        "target": "http://localhost:7446",
        "pathRewrite": {
          "^/ajax": ""
        },
        "xfwd": true,
        "secure": false,
        "changeOrigin": true
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "~7.4.5",
    "@medux/react-web-router": "~0.2.8",
    "antd": "~3.23.1",
    "babel-polyfill": "~6.26.0",
    "core-js": "~3.1.3",
    "react": "~16.8.6",
    "react-dom": "~16.8.6"
  },
  "devDependencies": {
    "@medux/dev-pkg": "~0.2.8",
    "@medux/dev-utils": "~0.2.8",
    "@types/react": "^16.8.9",
    "@types/react-dom": "^16.8.9",
    "babel-plugin-import": "~1.12.0",
    "less": "~3.10.3",
    "less-loader": "~5.0.0",
    "typescript": "~3.5.3"
  }
}
